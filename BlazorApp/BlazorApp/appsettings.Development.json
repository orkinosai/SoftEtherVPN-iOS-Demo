{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AppSettings": {
    "UseMockData": true,
    "MockDataSettings": {
      "AccommodationFormsEnabled": true,
      "MaxFormsPerUser": 10,
      "DefaultLocation": "Development Environment"
    },
    "ApplicationTitle": "Azure Accommodation Application Form (Development)",
    "Features": {
      "EnableNotifications": true,
      "EnableReporting": true,
      "EnableAdvancedSearch": true
    }
  },
  "ApplicationSettings": {
    "ApplicationName": "Azure Accommodation Form (Development)",
    "ApplicationUrl": "https://localhost:7091/",
    "TokenExpirationMinutes": 30,
    "TokenLength": 6
  },
  "EmailSettings": {
    "CompanyEmail": "dev@localhost.com",
    "FromEmail": "noreply@localhost.com",
    "FromName": "Azure Accommodation Form (Dev)",
    "SmtpPassword": "dev-password",
    "SmtpPort": 587,
    "SmtpServer": "localhost",
    "SmtpUsername": "dev@localhost.com",
    "UseSsl": false
  },
  "BlobStorageSettings": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "ContainerName": "dev-form-submissions"
  },
  "ApplicationInsights": {
    "ConnectionString": "",
    "AgentExtensionVersion": "~2",
    "Mode": "development"
  },
  "Diagnostics": {
    "AzureBlobRetentionInDays": 1,
    "WebsiteHttpLoggingRetentionDays": 1
  }
}
